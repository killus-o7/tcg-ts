{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build"
  },
  "paths": {
    "resources/*": ["$/*"],
    "classes/*": ["src/classes/*"],
    "handlers/*": ["src/handlers/*"],
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}
